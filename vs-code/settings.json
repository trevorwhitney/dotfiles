{
  "editor.fontFamily": "JetBrains Mono, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.hover.enabled": false,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.suggestSelection": "first",
  "editor.tokenColorCustomizations": {
    "[Gruvbox Light Hard]": {
      "textMateRules": [
        {
          "scope": "keyword",
          "settings": {
            "fontStyle": "bold",
            "foreground": "#665C54"
          }
        },
        {
          "scope": "keyword.control",
          "settings": {
            "fontStyle": "bold",
            "foreground": "#665C54"
          }
        }
      ]
    }
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.idea": true,
    "**/.project": true,
    "**/.settings": true
  },
  "go.toolsManagement.autoUpdate": true,
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--fast"
  ],
  "vim.easymotion": true,
  "vim.enableNeovim": true,
  "vim.foldfix": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-d>": false,
    "<C-j>": true,
    "<C-k>": true,
    "<C-r>": true
  },
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "after": [
        "<Esc>"
      ],
      "before": [
        "j",
        "j"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.neovimConfigPath": "$HOME/.config/nvim/init.vim",
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.neovimUseConfigFile": true,
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<C-j>"
      ],
      "commands": [
        "editor.action.smartSelect.shrink"
      ]
    },
    {
      "before": [
        "<C-k>"
      ],
      "commands": [
        "editor.action.smartSelect.expand"
      ]
    },
    {
      "before": [
        "<leader>",
        "<enter>"
      ],
      "commands": [
        "editor.action.quickFix"
      ]
    },
    {
      "before": [
        "<C-t>"
      ],
      "commands": [
        "editor.action.refactor"
      ]
    },
    {
      "before": [
        "<leader>",
        "="
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "t"
      ],
      "commands": [
        "go.test.cursor"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "l"
      ],
      "commands": [
        "go.test.previous"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "r"
      ],
      "commands": [
        "go.test.previous"
      ]
    },
    {
      "before": [
        "<leader>",
        "d",
        "t"
      ],
      "commands": "go.debug.cursor"
    },
    {
      "before": [
        "<leader>",
        "h"
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "r"
      ],
      "commands": [
        "editor.action.startFindReplaceAction"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "R"
      ],
      "commands": [
        "workbench.action.replaceInFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "t"
      ],
      "commands": [
        "go.test.generate.function"
      ]
    },
    {
      "before": [
        "<leader>",
        "i"
      ],
      "commands": [
        "go.impl.cursor"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "e"
      ],
      "commands": [
        "editor.action.rename"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "\\",
        "e"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "\\",
        "o"
      ],
      "commands": [
        "outline.focus"
      ]
    },
    {
      "before": [
        "\\",
        "w"
      ],
      "commands": [
        "workbench.action.closeEditorsInGroup"
      ]
    },
    {
      "after": [
        "O",
        "<esc>",
        "j"
      ],
      "before": [
        "[",
        "<space>"
      ]
    },
    {
      "after": [
        "o",
        "<esc>",
        "k"
      ],
      "before": [
        "]",
        "<space>"
      ]
    },
    {
      "before": [
        "[",
        "b"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
    {
      "before": [
        "]",
        "b"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
    {
      "before": [
        "[",
        "g"
      ],
      "commands": [
        "editor.action.marker.previous"
      ]
    },
    {
      "before": [
        "]",
        "g"
      ],
      "commands": [
        "editor.action.marker.next"
      ]
    },
    {
      "before": [
        "[",
        "c"
      ],
      "commands": [
        "workbench.action.editor.previousChange"
      ]
    },
    {
      "before": [
        "]",
        "c"
      ],
      "commands": [
        "workbench.action.editor.nextChange"
      ]
    },
    {
      "before": [
        "g",
        "i"
      ],
      "commands": [
        "editor.action.goToImplementation"
      ]
    },
    {
      "before": [
        "g",
        "y"
      ],
      "commands": [
        "editor.action.goToImplementation"
      ]
    },
    {
      "before": [
        "g",
        "t"
      ],
      "commands": [
        "go.toggle.test.file"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.goToReferences"
      ]
    },
    {
      "before": [
        "\\",
        "b"
      ],
      "commands": [
        "editor.debug.action.toggleBreakpoint"
      ]
    },
    {
      "before": [
        "\\",
        "B"
      ],
      "commands": [
        "editor.debug.action.conditionalBreakpoint"
      ]
    }
  ],
  "vim.operatorPendingModeKeyBindings": [],
  "vim.replaceWithRegister": true,
  "vim.showMarksInGutter": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "$HOME/.vim/config.vim",
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "<C-j>"
      ],
      "commands": [
        "editor.action.smartSelect.shrink"
      ]
    },
    {
      "before": [
        "<C-k>"
      ],
      "commands": [
        "editor.action.smartSelect.expand"
      ]
    },
    {
      "before": [
        "<leader>",
        "<enter>"
      ],
      "commands": [
        "editor.action.quickFix"
      ]
    },
    {
      "before": [
        "<C-t>"
      ],
      "commands": [
        "editor.action.refactor"
      ]
    },
    {
      "before": [
        "<leader>",
        "="
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "e"
      ],
      "commands": [
        "editor.action.rename"
      ]
    }
  ],
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "Gruvbox Light Hard",
  "workbench.editor.showTabs": false,
  "workbench.iconTheme": "vscode-icons",
  "workbench.preferredDarkColorTheme": "Gruvbox Light Hard",
  "workbench.preferredLightColorTheme": "Gruvbox Light Hard",
  "files.autoSaveDelay": 5000,
  "editor.formatOnSave": true,
  "[go]": {
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "go.formatFlags": [
    "-local=github.com/grafana/backend-enterprise"
  ]
}
