[
  {
    "command": "workbench.action.compareEditor.previousChange",
    "key": "[ c",
    "when": "textCompareEditorVisible"
  },
  {
    "command": "workbench.files.action.focusOpenEditorsView",
    "key": "\\ e",
    "when": "sideBarVisible && !editorTextFocus"
  },
  {
    "command": "workbench.action.findInFiles",
    "key": "\\ f",
    "when": "sideBarVisible && !editorTextFocus"
  },
  {
    "command": "workbench.view.scm",
    "key": "\\ g",
    "when": "sideBarVisible && !editorTextFocus"
  },
  {
    "command": "outline.focus",
    "key": "\\ o",
    "when": "sideBarVisible && !editorTextFocus"
  },
  {
    "command": "workbench.explorer.fileView.focus",
    "key": "\\ p",
    "when": "sideBarVisible && !editorTextFocus"
  },
  {
    "command": "workbench.view.debug",
    "key": "\\ r",
    "when": "sideBarVisible && !editorTextFocus"
  },
  {
    "command": "workbench.action.compareEditor.nextChange",
    "key": "] c",
    "when": "textCompareEditorVisible"
  },
  {
    "command": "workbench.action.moveEditorToLeftGroup",
    "key": "alt+h",
    "when": "editorTextFocus"
  },
  {
    "command": "workbench.action.moveEditorToRightGroup",
    "key": "alt+l",
    "when": "editorTextFocus"
  },
  {
    "command": "-togglePreserveCase",
    "key": "alt+p",
    "when": "editorFocus"
  },
  {
    "command": "editor.action.triggerParameterHints",
    "key": "alt+p",
    "when": "editorHasSignatureHelpProvider && editorTextFocus"
  },
  {
    "command": "workbench.action.splitEditorOrthogonal",
    "key": "ctrl+a -"
  },
  {
    "command": "workbench.action.toggleSidebarVisibility",
    "key": "ctrl+a /",
    "when": "sideBarVisible && editorTextFocus"
  },
  {
    "args": {
      "sequence": [
        "workbench.action.toggleSidebarVisibility",
        "workbench.action.focusSideBar"
      ]
    },
    "command": "extension.multiCommand.execute",
    "key": "ctrl+a \\",
    "when": "!sideBarFocus && !sideBarVisible"
  },
  {
    "command": "workbench.action.focusSideBar",
    "key": "ctrl+a \\",
    "when": "!sideBarFocus && sideBarVisible"
  },
  {
    "command": "workbench.action.toggleSidebarVisibility",
    "key": "ctrl+a \\",
    "when": "sideBarFocus"
  },
  {
    "command": "workbench.debug.action.toggleRepl",
    "key": "ctrl+a d",
    "when": "workbench.panel.repl.view.active"
  },
  {
    "command": "workbench.files.action.focusOpenEditorsView",
    "key": "ctrl+a e",
    "when": "workbench.explorer.openEditorsView.active"
  },
  {
    "command": "workbench.action.findInFiles",
    "key": "ctrl+a f"
  },
  {
    "command": "workbench.view.scm",
    "key": "ctrl+a g",
    "when": "workbench.scm.active"
  },
  {
    "command": "workbench.action.focusLeftGroup",
    "key": "ctrl+a h"
  },
  {
    "command": "workbench.action.focusBelowGroup",
    "key": "ctrl+a j"
  },
  {
    "command": "workbench.action.focusAboveGroup",
    "key": "ctrl+k",
    "when": "panelFocus && !editorTextFocus"
  },
  {
    "command": "workbench.action.focusRightGroup",
    "key": "ctrl+l",
    "when": "sideBarVisible && !editorTextFocus"
  },
  {
    "command": "goToNextReference",
    "key": "ctrl+a n",
    "when": "inReferenceSearchEditor || referenceSearchVisible"
  },
  {
    "command": "search.action.focusNextSearchResult",
    "key": "ctrl+a n",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "command": "references-view.next",
    "key": "ctrl+a n",
    "when": "reference-list.hasResult && references-view.canNavigate"
  },
  {
    "command": "goToPreviousReference",
    "key": "ctrl+a p",
    "when": "inReferenceSearchEditor || referenceSearchVisible"
  },
  {
    "command": "search.action.focusPreviousSearchResult",
    "key": "ctrl+a p",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "command": "references-view.prev",
    "key": "ctrl+a p",
    "when": "reference-list.hasResult && references-view.canNavigate"
  },
  {
    "command": "workbench.action.output.toggleOutput",
    "key": "ctrl+a r",
    "when": "workbench.panel.output.active"
  },
  {
    "command": "workbench.action.splitEditor",
    "key": "ctrl+a shift+\\"
  },
  {
    "command": "workbench.action.terminal.toggleTerminal",
    "key": "ctrl+a t",
    "when": "terminal.active"
  },
  {
    "command": "editor.debug.action.toggleBreakpoint",
    "key": "ctrl+f8",
    "when": "debuggersAvailable && editorTextFocus"
  },
  {
    "command": "workbench.action.quickOpenNavigateNext",
    "key": "ctrl+n",
    "when": "inQuickOpen"
  },
  {
    "command": "workbench.action.quickOpenNavigatePrevious",
    "key": "ctrl+p",
    "when": "inQuickOpen"
  },
  {
    "command": "workbench.action.toggleMultiCursorModifier",
    "key": "ctrl+shift+8"
  },
  {
    "command": "editor.debug.action.conditionalBreakpoint",
    "key": "ctrl+shift+f8"
  },
  {
    "command": "workbench.action.debug.restart",
    "key": "ctrl+shift+f9",
    "when": "inDebugMode"
  },
  {
    "command": "git.openFile",
    "key": "ctrl+shift+g o"
  },
  {
    "command": "editor.action.insertCursorBelow",
    "key": "ctrl+shift+j",
    "when": "editorTextFocus"
  },
  {
    "command": "editor.action.insertCursorAbove",
    "key": "ctrl+shift+k",
    "when": "editorTextFocus"
  },
  {
    "command": "workbench.action.debug.stepInto",
    "key": "f7",
    "when": "debugState != 'inactive'"
  },
  {
    "command": "workbench.action.debug.stepOver",
    "key": "f8",
    "when": "debugState == 'stopped'"
  },
  {
    "command": "workbench.action.debug.continue",
    "key": "f9",
    "when": "inDebugMode"
  },
  {
    "command": "editor.action.rename",
    "key": "shift+f6",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "command": "workbench.action.debug.stepOut",
    "key": "shift+f7",
    "when": "debugState == 'stopped'"
  },
  {
    "key": "ctrl+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "ctrl+shift+n",
    "command": "-workbench.action.newWindow"
  },
  {
    "key": "ctrl+shift+n",
    "command": "workbench.action.files.newUntitledFile"
  },
  {
    "key": "ctrl+n",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+p",
    "command": "selectPreviousSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+n",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+n",
    "command": "list.focusDown",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "ctrl+p",
    "command": "list.focusUp",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.focusRightGroup"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.focusLeftGroup"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.quickOpen",
    "when": "sideBarFocus"
  },
  {
    "key": "ctrl+j",
    "command": "-workbench.action.togglePanel"
  }
]