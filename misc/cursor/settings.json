    "vim.useSystemClipboard": true,
    "vim.handleKeys": {
        "<C-d>": true,
        "<C-r>": true,
        "<C-s>": false,
        "<C-z>": false
    },
    "cursor.composer.textSizeScale": 1.3,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "editor.wordWrap": "on",
    "files.autoSave": "onFocusChange",
    "git.autofetch": true,
    "git.confirmSync": false,
    "gitlens.ai.gitkraken.model": "gemini:gemini-2.0-flash",
    "gitlens.ai.model": "gitkraken",
    "nix.enableLanguageServer": true,
    "nix.formatterPath": "nixpkgs-fmt",
    "nix.serverPath": "nil",
    "nix.serverSettings": {
        "nil": {
            "formatting": {
                "command": [
                    "nixpkgs-fmt"
                ]
            }
        }
    },
    "redhat.telemetry.enabled": true,
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
    "window.autoDetectColorScheme": true,
    "window.openFoldersInNewWindow": "on",
    "window.systemColorTheme": "auto",
    "workbench.activityBar.orientation": "vertical",
    "workbench.colorTheme": "Everforest Light",
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.editor.showTabs": "single",
    "workbench.preferredDarkColorTheme": "Everforest Dark",
    "workbench.preferredLightColorTheme": "Everforest Light",
    "zenMode.centerLayout": false,
    "zenMode.fullScreen": false,
    "zenMode.hideActivityBar": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.hideStatusBar": false,
    "zenMode.restore": true,
    "zenMode.showTabs": "none",
    "extensions.ignoreRecommendations": true,
    "vim.highlightedyank.enable": true,
    "everforest.lightContrast": "hard",
    "everforest.darkContrast": "hard",
    "files.exclude": {
        "**/.direnv": true
    },
    "diffEditor.codeLens": true,
    "debug.inlineValues": "on"
}